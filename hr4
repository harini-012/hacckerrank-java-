import java.util.*;
class Node
{
    int task_id;
    Node next;
    public Node(int task_id)
    {
        this.task_id=task_id;
        this.next=null;
    }
}
public class Solution 
{
    Node head;
   void insertAtEnd(int task_id)
   {
       Node newNode=new Node(task_id);
       if(head==null)
       {
           head=newNode;
           return;
       }
       Node temp=head;
       while(temp.next!=null)
       {
           temp=temp.next;
       }
       temp.next=newNode;
   }
    void deleteAtPosition(int pos)
    {
        if(head==null)
        {
            return;
        }
        if(pos<0)
        {
            return;
        }
        if(pos==0)
        {
            head=head.next;
            return;
        }
        else
        {
            Node temp=head;
            int index=0;
            while(temp!=null && index<pos-1)
            {
                temp=temp.next;
                index++;
            }
            if(temp==null||temp.next==null)
            {
                return;
            }
            temp.next=temp.next.next;
        }
    }
    void display()
    {
        if(head==null)
        {
            System.out.println("List is empty");
            return;
        }
        Node temp=head;
        while(temp!=null)
        {
            System.out.print(temp.task_id+" ");
            temp=temp.next;
            
        }
    }
    public static void main(String[] args) 
    
    {
        Solution dp=new Solution();
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=0;i<n;i++)
        {
            int task_id=sc.nextInt();
            dp.insertAtEnd(task_id);
        }
        int pos=sc.nextInt();
        dp.deleteAtPosition(pos-1);
        dp.display();
}
}
