import java.util.*;
class Node
{
    int task_id;
    Node next;
    public Node(int task_id)
    {
        this.task_id=task_id;
        this.next=null;
    }
}

public class Solution 
{
    Node head;
    void insertAtEnd(int task_id)
    {
        Node newNode=new Node(task_id);
        if(head==null)
        {
            head=newNode;
            return;
        }
        Node temp=head;
        while(temp.next!=null)
        {
            temp=temp.next;
        }
        temp.next=newNode;
    }
    void display()
    {
        if(head==null)
        {
            return;
        }
        int count=0;
        Node temp=head;
        while(temp!=null)
        {
            count++;
            temp=temp.next;
        }
        
        int midIndex=count/2;
        temp=head;
        for(int i=0;i<midIndex;i++)
        {
            temp=temp.next;
        }
        while(temp!=null)
        {
            System.out.print(temp.task_id+" ");
            temp=temp.next;
        }
        
    }
    
    
    public static void main(String[] args) 
    {
        Solution md=new Solution();
        Scanner sc=new Scanner(System.in);
        int no=sc.nextInt();
        for(int i=0;i<no;i++)
        {
            int task_id=sc.nextInt();
            md.insertAtEnd(task_id);
        }
        md.display();
    }
}
