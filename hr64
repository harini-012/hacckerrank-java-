import java.io.*;
import java.util.*;
class Node
{
    int data;
    Node left,right=null;
    Node(int data)
    {
        this.left=null;
        this.right=null;
        this.data=data;
    }
}
public class Solution 
{
     static Queue<Node>q=new LinkedList<>();
    static Node buildTree(Node root,int value)
    {
        if(root==null)
        {
            return new Node(value);
        }
        if(value<root.data)
        {
            root.left=buildTree(root.left,value);
        }
        if(value>root.data)
        {
            root.right=buildTree(root.right,value);
        }
        return root;
    }
   
    static void inorder(Node root)
    {
        if(root==null)
        {
            return;
        }
        inorder(root.left);
        q.add(root);
        inorder(root.right);
       
    }

    public static void main(String[] args) 
    {
       Scanner sc=new Scanner(System.in);
       int no=sc.nextInt();
        Node root=null;
       for(int i=0;i<no;i++)
       {
           int value=sc.nextInt();
           root=buildTree(root,value);
       }
        inorder(root);
        System.out.println(q.poll().data);
        
        
        
    }
}
