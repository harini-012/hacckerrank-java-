import java.io.*;
import java.util.*;

public class Solution 
{
    static void dfs(int city,ArrayList<Integer>[]adj,boolean[]visited)
    {
        visited[city]=true;
        for(int c:adj[city])
        {
            if(!visited[c])
            {
                dfs(c,adj,visited);
            }
        }
    }
    static boolean allVisited(boolean[]visited)
    {
          for(int i = 1; i < visited.length; i++)
        {
            if(!visited[i])
            {
                return false;
            }
        }
        return true;
    }
    public static void main(String[] args) 
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int m=sc.nextInt();
        ArrayList<Integer>[]adj=new ArrayList[n+1];
        ArrayList<Integer>[]reversed=new ArrayList[n+1];
        for(int i=1;i<=n;i++)
        {
            adj[i]=new ArrayList<>();
        }
         for(int i=1;i<=n;i++)
        {
            reversed[i]=new ArrayList<>();
        }
        for(int i=0;i<m;i++)
        {
            int value1=sc.nextInt();
            int value2=sc.nextInt();
            adj[value1].add(value2);
           reversed[value2].add(value1);
        }
        boolean[]visited=new boolean[n+1];
        dfs(1,adj,visited);
        
        if(!allVisited(visited))
        {
            System.out.println("The road network is not connected.");
            return;
        }
        Arrays.fill(visited,false);
        dfs(1,reversed,visited);
        if(!allVisited(visited))
        {
            System.out.println("The road network is not connected.");
           
        }
        else
        {
            System.out.println("The road network is connected.");
        }
       
        
        
        
    }
}
