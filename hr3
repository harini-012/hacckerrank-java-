import java.util.*;
class Node
{
    int task_id;
    Node next;
    public Node(int task_id)
    {
        this.task_id=task_id;
        this.next=null;
    }
}

public class Solution 
{
   Node head;
    void insertAtEnd(int task_id)
    {
        Node newNode=new Node(task_id);
        if(head==null)
        {
            head=newNode;
        }
        else
        {
            Node temp=head;
            while(temp.next!=null)
            {
                temp=temp.next;
            }
            temp.next=newNode;
        }
    }
    void insertAtPosition(int pos,int new_id)
    {
        Node newNode=new Node(new_id);
        if(pos<0)
        {
            return;
        }
        if(pos==0)
        {
            newNode.next=head;
            head=newNode;
        }
        else
        {
            int index=0;
            Node temp=head;
            while(temp!=null&&index<pos-1)
            {
                temp=temp.next;
                index++;
            }
            if(temp==null)
            {
                return;
            }
            newNode.next=temp.next;
            temp.next=newNode;
            
        }
    }
         void display()
    {
        if(head==null)
        {
            return;
        }
        Node temp=head;
        while(temp!=null)
        {
            System.out.print(temp.task_id+" ");
            temp=temp.next;
        }
    }
    public static void main(String[] args) 
    {
        Scanner sc=new Scanner(System.in);
       Solution ip=new Solution();
      int n=sc.nextInt();
        for (int i=0;i<n;i++)
        {
            int task_id=sc.nextInt();
            ip.insertAtEnd(task_id);
        }
        int pos=sc.nextInt();
        int new_id=sc.nextInt();
        ip.insertAtPosition((pos-1),new_id);
        ip.display();
    }
    }
