import java.util.*;
class Node
{
    int proId;
    Node next;
    public Node(int proId)
    {
        this.proId=proId;
        this.next=null;
    }
}
public class Solution
{
    Node head;
    void insertAtBegining(int proId)
    {
        Node newNode=new Node(proId);
        newNode.next=head;
        head=newNode;
    }
    void display()
    {
        if(head==null)
        {
            return;
        }
        Node temp=head;
        while(temp!=null)
        {
            System.out.print(temp.proId+" ");
            temp=temp.next;
        }
    }
     public static void main(String[]args)
    {
        Solution pi=new Solution();
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=0;i<n;i++)
        {
            int proId=sc.nextInt();
            pi.insertAtBegining(proId);
        }
        pi.display();
    }
}
